<% @result.patterns(params[:student_attributes]).each_with_index do |pattern, i| %>
  <div class="card mb-3">
    <div class="card-header">
      <%= pattern[:label] %>
    </div>
    <div class="card-body">
      <div class="row">
        <%= pie_chart pattern[:options] %>
        <%= pie_chart @survey.aggregated["patterns"]["#{@result.grade}-#{@result.subject}-#{params[:comparator]}"][i]["options"] %>
      </div>
    </div>
  </div>
<% end %>
